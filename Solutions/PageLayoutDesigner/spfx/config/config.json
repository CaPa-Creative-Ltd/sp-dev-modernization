{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "page-layout-designer-command-set": {
      "components": [
        {
          "entrypoint": "./lib/extensions/pageLayoutDesigner/PageLayoutDesignerCommandSet.js",
          "manifest": "./src/extensions/pageLayoutDesigner/PageLayoutDesignerCommandSet.manifest.json"
        }
      ]
    },
    "page-designer-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/pageDesigner/PageDesignerWebPart.js",
          "manifest": "./src/webparts/pageDesigner/PageDesignerWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "PageLayoutDesignerCommandSetStrings": "lib/extensions/pageLayoutDesigner/loc/{locale}.js",
    "PageDesignerWebPartStrings": "lib/webparts/pageDesigner/loc/{locale}.js",
    "PageDesignerComponentStrings": "lib/components/pageDesigner/loc/{locale}.js"
  }
}
