{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "transform-library": {
      "components": [
        {
          "entrypoint": "./lib/libraries/transform/TransformLibrary.js",
          "manifest": "./src/libraries/transform/TransformLibrary.manifest.json"
        }
      ]
    },
    "page-layout-designer-command-set": {
      "components": [
        {
          "entrypoint": "./lib/extensions/pageLayoutDesigner/PageLayoutDesignerCommandSet.js",
          "manifest": "./src/extensions/pageLayoutDesigner/PageLayoutDesignerCommandSet.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "TransformLibraryStrings": "lib/libraries/transform/loc/{locale}.js",
    "PageLayoutDesignerCommandSetStrings": "lib/extensions/pageLayoutDesigner/loc/{locale}.js"
  }
}
